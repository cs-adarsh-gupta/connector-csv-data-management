      "operation": "extract_data_from_csv",
      "description": "This action will extract data from CSV based on column names provided. User can select option to deduplicate rows based on columns",
      "parameters": [
        {
          "title": "Type",
          "type": "select",
          "name": "input",
          "required": true,
          "visible": true,
          "editable": true,
          "value": "",
          "options": [
            "Attachment IRI",
            "File IRI"
          ]
        },
        {
          "title": "Reference ID",
          "type": "text",
          "name": "value",
          "required": true,
          "visible": true,
          "editable": true,
          "value": ""
        },
        {
          "title": "Column Names",
          "type": "text",
          "name": "columnNames",
          "required": false,
          "visible": true,
          "editable": true,
          "value": ""
        },
        {
          "title": "Deduplicate Values on",
          "type": "text",
          "name": "deDupValuesOn",
          "required": false,
          "visible": true,
          "editable": true,
          "value": ""
